<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnEnableUserMatching"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/enable_user_matching"/>

    <Button
        android:id="@+id/btnDisableUserMatching"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/disable_user_matching"/>

    <Button
        android:id="@+id/btnCustomActionWithCustomUserId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/custom_action_with_custom_userid"/>

    <Button
        android:id="@+id/btnCustomActionWithMappUserId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/custom_action_with_mapp_userid"/>

    <Button
        android:id="@+id/btnSendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/send_data"/>
</androidx.appcompat.widget.LinearLayoutCompat>